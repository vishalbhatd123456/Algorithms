 int sum = 0;
        int count = 0;
        
        for(int i=0;i<n;i++)
        {
            sum+=a[i];
            if(sum == 0)
            {
                return 0;
            }
            count++;
            if(sum>k)
            {
                break;
            }
        }
        
        return count-1;
